{
  "name": "peer-feedback",
  "version": "1.0.0",
  "description": "This is an AI application for gathering contributions for an individual for a given time period and turning that data into feedback for the individual.",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node --loader ts-node/esm src/index.ts",
    "debug": "DEBUG=true node --loader ts-node/esm src/index.ts",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": [],
  "author": "Jonathan Hoyt <jonmagic@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@inngest/agent-kit": "^0.7.2",
    "@octokit/graphql": "^8.2.2",
    "@octokit/rest": "^21.1.1",
    "dotenv": "^16.5.0",
    "openai": "^4.94.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/eslint": "^8.56.12",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "jiti": "^2.4.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2"
  }
}
